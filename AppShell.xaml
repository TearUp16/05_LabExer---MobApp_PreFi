<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUIApp5.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUIApp5.Pages"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem
        FlyoutDisplayOptions="AsMultipleItems">

        <ShellContent
            Title="League of Legends"
            ContentTemplate="{DataTemplate local:LoLPage}"
            Route="League of Legends" 
             FlyoutIcon="lol.jpg"/>


        <ShellContent
            Title="Unity"
            ContentTemplate="{DataTemplate local:UnityPage}"
            Route="Unity" 
            FlyoutIcon="unity.png"/>

        <ShellContent
            Title="Valorant"
            ContentTemplate="{DataTemplate local:ValorantPage}"
            Route="Valorant" 
            FlyoutIcon="valorant.png"/>
        
    </FlyoutItem>


</Shell>
